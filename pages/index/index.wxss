.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  color: #aaa;
}

.section-start {
  margin-top: 200px;
}
.section-start button{
  background: #FFA54F;
  color: white;
  font-size: 28rpx;
  width: 240rpx;
  height: 60rpx;
  line-height: 60rpx;
}